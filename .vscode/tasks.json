{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build",
            "type": "process",
            "command": "${config:maven.executable.path}",
            "args": [
                "package"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        },
        {
            "label": "Rebuild",
            "type": "process",
            "command": "${config:maven.executable.path}",
            "args": [
                "clean",
                "package"
            ],
            "group": "build",
            "problemMatcher": []
        },
        {
            "label": "Install",
            "type": "process",
            "command": "${config:maven.executable.path}",
            "args": [
                "install",
            ],
            "group": "build",
            "problemMatcher": []
        },
        {
            "label": "Clean",
            "type": "process",
            "command": "${config:maven.executable.path}",
            "args": [
                "clean"
            ],
            "group": "none"
        }
    ]
}
